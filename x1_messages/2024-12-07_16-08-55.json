\u6211\u524d\u884c\u7684\u9053\u8def\u3002\n\n\u7ec8\u4e8e\uff0c\u5165\u573a\u7684\u65f6\u95f4\u5230\u4e86\u3002\u6211\u8ddf\u968f\u5176\u4ed6\u6f14\u5458\u8d70\u8fdb\u5e10\u7bf7\uff0c\u706f\u5149\u6e10\u6e10\u6697\u4e0b\uff0c\u89c2\u4f17\u7684\u55a7\u56a3\u58f0\u6162\u6162\u5e73\u606f\u3002\u4e3b\u6301\u4eba\u8d70\u4e0a\u53f0\uff0c\u5ba3\u5e03\u4eca\u665a\u7684\u8282\u76ee\u5f00\u59cb\u3002\u6211\u7684\u5fc3\u8df3\u51e0\u4e4e\u8981\u8df3\u51fa\u80f8\u8154\uff0c\u4f46\u6211\u5f3a\u8feb\u81ea\u5df1\u4fdd\u6301\u51b7\u9759\u3002\n\n\u7b2c\u4e00\u4e2a\u8282\u76ee\u662f\u6742\u6280\u8868\u6f14\uff0c\u63a5\u7740\u662f\u5c0f\u4e11\u7684\u6ed1\u7a3d\u620f\uff0c\u7136\u540e\u662f\u52a8\u7269\u8868\u6f14\u2026\u2026\u6bcf\u4e00\u4e2a\u8282\u76ee\u90fd\u8d62\u5f97\u4e86\u89c2\u4f17\u7684\u9635\u9635\u638c\u58f0\u548c\u559d\u5f69\u3002\u6211\u5750\u5728\u540e\u53f0\uff0c\u542c\u7740\u5916\u9762\u7684\u70ed\u95f9\uff0c\u65e2\u611f\u5230\u9ad8\u5174\uff0c\u53c8\u611f\u5230\u7126\u8651\u3002\u6211\u6e34\u671b\u4e0a\u53f0\uff0c\u4f46\u540c\u65f6\u53c8\u5bb3\u6015\u9762\u5bf9\u90a3\u9ad8\u9ad8\u7684\u94a2\u4e1d\u3002\n\n\u7ec8\u4e8e\uff0c\u8f6e\u5230\u6211\u4e86\u3002\u6211\u7ad9\u8d77\u6765\uff0c\u6df1\u5438\u4e00\u53e3\u6c14\uff0c\u8d70\u5411\u821e\u53f0\u3002\u5f53\u6211\u8d70\u5230\u5e55\u540e\uff0c\u770b\u5230\u94a2\u4e1d\u5728\u706f\u5149\u4e0b\u95ea\u70c1\u7740\u51b7\u51bd\u7684\u5149\u8292\uff0c\u6211\u4e0d\u7981\u5c4f\u4f4f\u4e86\u547c\u5438\u3002\u6211\u95ed\u4e0a\u773c\u775b\uff0c\u9ed8\u9ed8\u5730\u91cd\u590d\u7740\u6211\u7684\u52a8\u4f5c\u8981\u9886\uff0c\u8bd5\u56fe\u5c06\u81ea\u5df1\u8c03\u6574\u5230\u6700\u4f73\u72b6\u6001\u3002\n\n\u7136\u540e\uff0c\u6211\u542c\u5230\u4e3b\u6301\u4eba\u7684\u4ecb\u7ecd\u58f0\uff1a\u201c\u5973\u58eb\u4eec\u3001\u5148\u751f\u4eec\uff0c\u8bf7\u6b22\u8fce\u6211\u4eec\u7684\u7279\u9080\u5609\u5bbe\uff0c\u8457\u540d\u7684\u9ad8\u7a7a\u8d70\u94a2\u4e1d\u827a\u672f\u5bb6\u2014\u2014[\u4f60\u7684\u540d\u5b57]\uff01\u201d\u89c2\u4f17\u5e2d\u4e0a\u7206\u53d1\u51fa\u70ed\u70c8\u7684\u638c\u58f0\u548c\u6b22\u547c\u58f0\u3002\u6211\u6df1\u5438\u4e00\u53e3\u6c14\uff0c\u7741\u5f00\u773c\u775b\uff0c\u8fc8\u51fa\u4e86\u7b2c\u4e00\u6b65\u3002\n\n\u8d70\u4e0a\u94a2\u4e1d\u7684\u90a3\u4e00\u523b\uff0c\u4e16\u754c\u4eff\u4f5b\u9759\u6b62\u4e86\u3002\u6211\u611f\u5230\u811a\u4e0b\u7684\u94a2\u4e1d\u5728\u5fae\u5fae\u98a4\u6296\uff0c\u90a3\u662f\u6211\u5fc3\u8df3\u7684\u56de\u54cd\u3002\u6211\u96c6\u4e2d\u5168\u90e8\u6ce8\u610f\u529b\uff0c\u611f\u53d7\u7740\u94a2\u4e1d\u7684\u6bcf\u4e00\u4e1d\u9707\u52a8\uff0c\u8c03\u6574\u7740\u8eab\u4f53\u7684\u5e73\u8861\u3002\u4e00\u6b65\uff0c\u4e24\u6b65\uff0c\u6bcf\u4e00\u6b65\u90fd\u50cf\u662f\u5728\u5200\u5c16\u4e0a\u821e\u8e48\uff0c\u6bcf\u4e00\u6b65\u90fd\u5145\u6ee1\u4e86\u6311\u6218\u3002\n\n\u4f46\u6211\u4e0d\u80fd\u505c\u987f\uff0c\u4e0d\u80fd\u72b9\u8c6b\u3002\u6211\u5fc5\u987b\u4fdd\u6301\u524d\u8fdb\uff0c\u76f4\u5230\u7ec8\u70b9\u3002\u6211\u60f3\u8c61\u7740\u81ea\u5df1\u662f\u4e00\u7247\u53f6\u5b50\uff0c\u968f\u98ce\u8f7b\u76c8\u5730\u98d8\u8fc7\uff0c\u53c8\u50cf\u662f\u4e00\u53ea\u9e1f\u513f\uff0c\u5728\u7a7a\u4e2d\u81ea\u7531\u7ff1\u7fd4\u3002\u6211\u5c06\u6240\u6709\u7684\u6050\u60e7\u548c\u4e0d\u5b89\u629b\u8bf8\u8111\u540e\uff0c\u53ea\u4e13\u6ce8\u4e8e\u5f53\u4e0b\u7684\u6bcf\u4e00\u6b65\u3002\n\n\u6e10\u6e10\u5730\uff0c\u6211\u627e\u5230\u4e86\u8282\u594f\uff0c\u6211\u7684\u6b65\u4f10\u53d8\u5f97\u66f4\u52a0\u7a33\u5065\u3002\u6211\u80fd\u611f\u53d7\u5230\u89c2\u4f17\u7684\u76ee\u5149\u7d27\u968f\u5176\u540e\uff0c\u4ed6\u4eec\u7684\u547c\u5438\u4f3c\u4e4e\u4e0e\u6211\u540c\u6b65\u3002\u6211\u751a\u81f3\u80fd\u542c\u5230\u4e00\u4e9b\u4eba\u53d1\u51fa\u7684\u60ca\u53f9\u58f0\u548c\u9f13\u52b1\u7684\u8bdd\u8bed\u3002\u8fd9\u4e9b\u58f0\u97f3\u7ed9\u4e86\u6211\u5de8\u5927\u7684\u529b\u91cf\uff0c\u8ba9\u6211\u66f4\u52a0\u81ea\u4fe1\u5730\u524d\u884c\u3002\n\n\u7ec8\u4e8e\uff0c\u6211\u8d70\u5230\u4e86\u94a2\u4e1d\u7684\u53e6\u4e00\u7aef\uff0c\u7a33\u7a33\u5730\u7ad9\u5b9a\u3002\u6211\u5c55\u5f00\u53cc\u81c2\uff0c\u5411\u89c2\u4f17\u81f4\u610f\u3002\u987f\u65f6\uff0c\u5e10\u7bf7\u5185\u7206\u53d1\u51fa\u96f7\u9e23\u822c\u7684\u638c\u58f0\u548c\u6b22\u547c\u58f0\u3002\u6211\u611f\u5230\u4e00\u80a1\u96be\u4ee5\u8a00\u55bb\u7684\u559c\u60a6\u548c\u6210\u5c31\u611f\u6d8c\u4e0a\u5fc3\u5934\u3002\u6211\u505a\u5230\u4e86\uff01\u6211\u6210\u529f\u5730\u5b8c\u6210\u4e86\u8fd9\u6b21\u8868\u6f14\uff0c\u800c\u4e14\u611f\u89c9\u6bd4\u4ee5\u5f80\u4efb\u4f55\u65f6\u5019\u90fd\u8981\u597d\u3002\n\n\u6211\u8d70\u4e0b\u821e\u53f0\uff0c\u56de\u5230\u540e\u53f0\uff0c\u5176\u4ed6\u6f14\u5458\u7eb7\u7eb7\u4e0a\u6765\u795d\u8d3a\u6211\u3002\u4ed6\u4eec\u7684\u7b11\u5bb9\u548c\u62e5\u62b1\u8ba9\u6211\u611f\u5230\u65e0\u6bd4\u6e29\u6696\u3002\u6211\u77e5\u9053\uff0c\u65e0\u8bba\u7ed3\u679c\u5982\u4f55\uff0c\u6211\u4eec\u90fd\u662f\u4e00\u4e2a\u56e2\u961f\uff0c\u4e92\u76f8\u652f\u6301\uff0c\u5171\u540c\u8fdb\u6b65\u3002\n\n\u63a5\u4e0b\u6765\uff0c\u8fd8\u6709\u66f4\u591a\u7684\u8868\u6f14\u7b49\u5f85\u7740\u89c2\u4f17\u3002\u4f46\u6211\u5df2\u7ecf\u5b8c\u6210\u4e86\u6211\u7684\u90e8\u5206\uff0c\u6211\u611f\u5230\u4e00\u79cd\u91ca\u7136\u548c\u6ee1\u8db3\u3002\u4eca\u665a\uff0c\u6211\u4e0d\u4ec5\u5c55\u793a\u4e86\u6211\u7684\u6280\u80fd\uff0c\u66f4\u5c55\u73b0\u4e86\u6211\u7684\u52c7\u6c14\u548c\u51b3\u5fc3\u3002\u8fd9\u5c06\u662f\u6211\u804c\u4e1a\u751f\u6daf\u4e2d\u6c38\u8fdc\u96be\u5fd8\u7684\u4e00\u5929\u3002\n\n\u968f\u7740\u6700\u540e\u4e00\u4e2a\u8282\u76ee\u7ed3\u675f\uff0c\u5e10\u7bf7\u5185\u7684\u706f\u5149\u4eae\u8d77\uff0c\u89c2\u4f17\u4eec\u9646\u7eed\u79bb\u5f00\uff0c\u8138\u4e0a\u6d0b\u6ea2\u7740\u6ee1\u610f\u7684\u7b11\u5bb9\u3002\u6211\u7ad9\u5728\u4e00\u65c1\uff0c\u770b\u7740\u8fd9\u4e00\u5207\uff0c\u5fc3\u4e2d\u5145\u6ee1\u4e86\u611f\u6fc0\u3002\u611f\u6fc0\u8fd9\u4e2a\u821e\u53f0\uff0c\u611f\u6fc0\u6240\u6709\u7684\u89c2\u4f17\uff0c\u611f\u6fc0\u4e0e\u6211\u5e76\u80a9\u4f5c\u6218\u7684\u540c\u4f34\u4eec\u3002\n\n\u4eca\u665a\uff0c\u6211\u5b9e\u73b0\u4e86\u6211\u7684\u68a6\u60f3\u3002\u6211\u7ad9\u5728\u4e86\u805a\u5149\u706f\u4e0b\uff0c\u63a5\u53d7\u4e86\u5168\u4e16\u754c\u7684\u8d5e\u7f8e\u3002\u4f46\u8fd9\u53ea\u662f\u4e00\u4e2a\u65b0\u7684\u5f00\u59cb\uff0c\u6211\u77e5\u9053\uff0c\u8fd8\u6709\u66f4\u591a\u7684\u6311\u6218\u5728\u7b49\u5f85\u7740\u6211\u3002\u4f46\u6211\u5df2\u7ecf\u51c6\u5907\u597d\u4e86\uff0c\u56e0\u4e3a\u6211\u6709\u51b3\u5fc3\uff0c\u6709\u6bc5\u529b\uff0c\u66f4\u6709\u5bf9\u8fd9\u4efd\u4e8b\u4e1a\u7684\u65e0\u9650\u70ed\u7231\u3002\n\n\u65e5\u8bb0\uff0c\u4eca\u5929\u662f\u6211\u4eba\u751f\u4e2d\u6700\u7f8e\u597d\u7684\u4e00\u5929\u4e4b\u4e00\u3002\u6211\u5c06\u6c38\u8fdc\u94ed\u8bb0\u8fd9\u4e00\u523b\uff0c\u5b83\u5c06\u6fc0\u52b1\u6211\u7ee7\u7eed\u524d\u884c\uff0c\u65e0\u8bba\u672a\u6765\u7684\u8def\u6709\u591a\u4e48\u8270\u96be\u3002\u56e0\u4e3a\u6211\u76f8\u4fe1\uff0c\u53ea\u8981\u6211\u575a\u6301\u4e0d\u61c8\uff0c\u5c31\u6ca1\u6709\u4ec0\u4e48\u662f\u4e0d\u53ef\u80fd\u7684\u3002\n\n\u665a\u5b89\uff0c\u65e5\u8bb0\u3002\n\n\u7231\u4f60\u7684\uff0c\n\n[\u4f60\u7684\u540d\u5b57]"
    }
]